          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code is 200\", function () {\r",
              "  pm.response.to.have.status(200);\r",
              "});\r",
              "\r",
              "pm.test(\"Response has expected keys\", function () {\r",
              "  const json = pm.response.json();\r",
              "  pm.expect(json).to.have.property(\"userId\");\r",
              "  pm.expect(json).to.have.property(\"id\");\r",
              "  pm.expect(json).to.have.property(\"title\");\r",
              "  pm.expect(json).to.have.property(\"completed\");\r",
              "});\r",
              "\r",
              "pm.test(\"Todo id is 1\", function () {\r",
              "  const json = pm.response.json();\r",
              "  pm.expect(json.id).to.eql(1);\r",
              "});\r",
              ""
